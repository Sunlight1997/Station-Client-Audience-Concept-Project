# Supabase Local Development Environment Variables
# Copy this file to .env and update the values

# Database
POSTGRES_PASSWORD=your-super-secret-password

# Auth
SUPABASE_AUTH_EXTERNAL_GITHUB_CLIENT_ID=your-github-client-id
SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET=your-github-secret

# Storage
SUPABASE_STORAGE_S3_ACCESS_KEY_ID=your-s3-access-key
SUPABASE_STORAGE_S3_SECRET_ACCESS_KEY=your-s3-secret-key
SUPABASE_STORAGE_S3_BUCKET=your-s3-bucket
SUPABASE_STORAGE_S3_REGION=your-s3-region

# Email (for testing)
SUPABASE_AUTH_SMS_TWILIO_AUTH_TOKEN=your-twilio-token

# Captcha
SUPABASE_AUTH_CAPTCHA_SECRET=your-captcha-secret
